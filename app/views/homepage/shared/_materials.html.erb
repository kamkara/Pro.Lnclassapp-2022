<div class="mx-4 my-2 p-2 bg-white">
    <% @feed_materials.each do |fm| %>
        <ul class="font-bold my-2 flex">
            <il class="flex ">           
                <p class="flex">
                    <span class="mx-2">
                        <%# if fm.materialIcon.attached? %> 
                            <%#= image_tag(fm.materialIcon.variant(resize_to_limit: [32, 32]), size: '32') %>
                        <%# end %>
                    </span>
                    <span class="mt-2">
                        <%= link_to fm.title, fm, data:{ turbo_frame: "material_feed" }, class:"no-underline font-semibold text-black" %>
                    </span>
                </p>
            </il>
        </ul>
    <% end %>
</div>


