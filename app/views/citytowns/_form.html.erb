<%= form_with(model: citytown, class: "contents") do |form| %>
<%= form_error_notification(citytown) %>

<div class="my-5">
    <%= form.label :title %>
    <%= form.text_field :title, autofocus:true, class: "block rounded-md border border-gray-200 outline-none px-3 py-2 mt-2 w-full" %>
  </div>

  
  <div class="inline">
    <%= form.submit class: "rounded-lg py-3 px-5 bg-blue-600 text-white inline-block font-medium cursor-pointer" %>
  </div>
<% end %>
